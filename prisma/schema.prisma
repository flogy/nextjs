generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Paths {
  id              Int @id @default(autoincrement()) //fixme: replace with uuid
  leistungsgruppe String
  name            String
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
